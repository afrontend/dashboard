{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Local web service for showing bookmarks",
  "repository": "https://github.com/afrontend/dashboard.git",
  "author": "afrontend <autoscripts@gmail.com>",
  "scripts": {
    "copyjson": "cp -rf json dist/",
    "build": "parcel build index.html --no-cache",
    "serve": "parcel index.html --no-cache",
    "watch": "parcel watch index.html --no-cache",
    "predeploy": "npm run copyjson && parcel build index.html",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src components hooks js",
    "lint:fix": "eslint src components hooks js --fix",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@parcel/packager-raw-url": "2.16.1",
    "@parcel/transformer-webmanifest": "2.16.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^15.12.0",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "gh-pages": "^5.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.0"
  }
}
